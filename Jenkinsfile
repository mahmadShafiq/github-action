pipeline{
    agent any
    stages{
         stage("Build")
        {
            steps{
                echo "Hi This is Build Environment.."
                echo "O Lala O Lala...."
                }
        }
         stage("Test")
        {
             steps{
                echo "Hi This is Test Environment.."
                 echo "Ab Mein Jawa Ho Gayee"
            }
        }
         stage("Deploy")
        {
              steps{
                echo "Hi This is Build Environment.."
            }
        }
    }
}
